{
	"name": "@lowcodeunit/theme-builder",
	"version": "0.0.1",
	"scripts": {
		"build:common": "ng build common",
		"build:demo": "ng build demo --prod",
		"build:lcu": "ng build lcu --prod --single-bundle && npm run pack",
		"demo": "npm run build:common && npm run start:demo",
		"demo:lcu": "npm run build:common && npm run build:lcu && npm run start:demo",
		"deploy": "npm version patch && npm run deploy:all",
		"deploy:all": "npm run deploy:common && npm run deploy:lcu && npm run deploy:demo",
		"deploy:common": "npm version patch --prefix projects/common && npm run build:common && npm publish ./dist/common --access public",
		"deploy:demo": "npm version patch --prefix projects/demo && npm run build:demo && npm publish ./dist/demo --access public",
		"deploy:lcu": "npm version patch --prefix projects/lcu && npm run build:lcu && npm publish ./dist/lcu --access public",
		"ng": "ng",
		"pack": "npm run pack:lcu",
		"pack:join": "concat-glob-cli -f \"dist/lcu/wc/lcu.*.js\" -o dist/lcu/wc/theme-builder.lcu.js",
		"pack:lcu": "rimraf dist/lcu/wc && mkdirp dist/lcu/wc && npm run pack:main && npm run pack:pollyfills && npm run pack:join",
		"pack:main": "concat-glob-cli -f \"dist/lcu/main-es2015.*.js\" -o dist/lcu/wc/lcu.startup.js",
		"pack:pollyfills": "concat-glob-cli -f \"dist/lcu/scripts.*.js\" -o dist/lcu/wc/lcu.pollyfills.js",
		"start": "ng serve demo --port=4227",
		"build": "npm run build:common && npm run build:lcu",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e"
	},
	"private": false,
	"dependencies": {
		"@angular/animations": "^9.0.2",
		"@angular/cdk": "^9.0.1",
		"@angular/common": "^9.0.2",
		"@angular/compiler": "^9.0.2",
		"@angular/core": "^9.0.2",
		"@angular/elements": "^9.0.2",
		"@angular/flex-layout": "^9.0.0-beta.29",
		"@angular/forms": "^9.0.2",
		"@angular/material": "^9.0.1",
		"@angular/platform-browser": "^9.0.2",
		"@angular/platform-browser-dynamic": "^9.0.2",
		"@angular/router": "^9.0.2",
		"@aspnet/signalr": "^1.1.4",
		"@lcu/common": "^1.1.31-angular-9",
		"@lowcodeunit-devkit/ng-lcu": "^0.86.439",
		"@types/highlight.js": "^9.12.3",
		"@types/tinycolor2": "^1.4.2",
		"@webcomponents/custom-elements": "^1.3.2",
		"@webcomponents/webcomponentsjs": "^2.4.1",
		"copy": "^0.3.2",
		"hammerjs": "^2.0.8",
		"highlight.js": "^9.18.1",
		"ngx-build-plus": "^9.0.6",
		"ngx-color-picker": "^9.0.0",
		"rxjs": "^6.5.4",
		"sass.js": "^0.11.1",
		"terser": "^4.6.3",
		"tinycolor2": "^1.4.1",
		"tslib": "^1.11.0",
		"zone.js": "~0.10.2"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^0.900.3",
		"@angular-devkit/build-ng-packagr": "~0.900.3",
		"@angular/cli": "~9.0.3",
		"@angular/compiler-cli": "^9.0.2",
		"@angular/language-service": "^9.0.2",
		"@lcu/cli": "^1.2.9",
		"@lowcodeunit/lcu-documentation-common": "^1.2.12",
		"@types/jasmine": "~3.3.8",
		"@types/jasminewd2": "~2.0.3",
		"@types/node": "^13.7.4",
		"@types/tinycolor2": "^1.4.1",
		"codelyzer": "^5.2.1",
		"concat-glob-cli": "^0.1.0",
		"cpx": "^1.5.0",
		"jasmine-core": "~3.4.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~4.1.0",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "~2.0.1",
		"karma-jasmine": "~2.0.1",
		"karma-jasmine-html-reporter": "^1.5.1",
		"mkdirp": "^0.5.1",
		"ng-momentum": "0.0.24",
		"ng-packagr": "^9.0.1",
		"protractor": "^5.4.3",
		"rimraf": "^2.6.3",
		"scss-to-json": "^2.0.0",
		"ts-node": "~7.0.0",
		"tsickle": "^0.38.0",
		"tslint": "~5.15.0",
		"typed-scss-modules": "^1.2.0",
		"typescript": "~3.7.5"
	}
}
